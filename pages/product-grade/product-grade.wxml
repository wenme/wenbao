<!--pages/product-grade/product-grade.wxml-->
<view class="padding-bottom">
    <view class="pro-company-name posi-fixed-top"><view class="productname padd-lr-30" >{{product}}</view></view>
    <view class="part-white padd-lr-30 padding-bot20 margin-top120">
		
        <view class="company-info-row posi-rela">
            <view class="gradeable">产品综合评分</view>
            <view class="gradecon">{{product_score}}分</view>
			<view class="mess-iconaa" bindtap="toExplaination" data-tid="{{product_score_hintid}}"></view>
        </view>
        <view class="company-info-row posi-rela">
            <view class="gradeable">产品综合评级</view>
            <view class="gradecon"><image src="https://resource.wenme.cc/static/images/star{{product_rating}}.png" ></image></view>
			<view class="mess-iconaa" bindtap="toExplaination" data-tid="{{product_rating_hintid}}"></view>
        </view>
		
		
	    <view class="clear"></view>
	</view>

    


    <view class="pro-detial-mess">
        <view class="pro-detial-mess-tit">
            <view class="proli-item">保险责任</view>
        </view>

        <view class="padd-lr-30">

            <view class="pro-detial-mess-row" wx:if="{{life_benefit_count}}">
                <view class="pro-duty-aa"><image src="https://resource.wenme.cc/static/images/insur-icona.png" ></image></view>
                <view class="pro-duty-bb">人寿</view>
                <view class="pro-duty-cc" data-code="1" bindtap="toZeren">{{life_benefit_count}}项保险金</view>
                <view class="clear"></view>
            </view>

            <view class="pro-detial-mess-row" wx:if="{{disease_benefit_count}}">
                <view class="pro-duty-aa"><image src="https://resource.wenme.cc/static/images/insur-iconb.png" ></image></view>
                <view class="pro-duty-bb">疾病</view>
                <view class="pro-duty-cc" data-code="2" bindtap="toZeren">{{disease_benefit_count}}项保险金</view>
                <view class="clear"></view>
            </view>

            <view class="pro-detial-mess-row" wx:if="{{accident_benefit_count}}">
                <view class="pro-duty-aa"><image src="https://resource.wenme.cc/static/images/insur-iconf.png" ></image></view>
                <view class="pro-duty-bb">意外</view>
                <view class="pro-duty-cc" data-code="3" bindtap="toZeren">{{accident_benefit_count}}项保险金</view>
                <view class="clear"></view>
            </view>

            <view class="pro-detial-mess-row" wx:if="{{medical_benefit_count}}">
                <view class="pro-duty-aa"><image src="https://resource.wenme.cc/static/images/insur-icond.png" ></image></view>
                <view class="pro-duty-bb">医疗</view>
                <view class="pro-duty-cc" data-code="4" bindtap="toZeren">{{medical_benefit_count}}项保险金</view>
                <view class="clear"></view>
            </view>
			
			<view class="pro-detial-mess-row" wx:if="{{saving_benefit_count}}">
                <view class="pro-duty-aa"><image src="https://resource.wenme.cc/static/images/insur-icone.png" ></image></view>
                <view class="pro-duty-bb">储蓄</view>
                <view class="pro-duty-cc" data-code="5" bindtap="toZeren">{{saving_benefit_count}}项保险金</view>
                <view class="clear"></view>
            </view>

            <view class="clear"></view>
        </view>

        <view class="clear"></view>
    </view>



    <!--<view class="pro-detial-mess" wx:if="{{product_structure!=='传统型'}}">-->
        <!--<view class="pro-detial-mess-tit">-->
            <!--<view class="proli-item">投资</view>-->
        <!--</view>-->

        <!--<view class="padd-lr-30">-->

            <!--<view class="pro-detial-mess-row">-->
                <!--<view class="pro-duty-bb">{{product_structure}}</view>-->
                <!--<view class="pro-duty-cc" bindtap="toProductType"></view>-->
                <!--<view class="clear"></view>-->
            <!--</view>-->

            <!---->
            <!--<view class="clear"></view>-->
        <!--</view>-->

        <!--<view class="clear"></view>-->
    <!--</view>-->

    <view class="pro-detial-mess">
        <view class="pro-detial-mess-tit">
            <view class="proli-item">重要条款</view>
        </view>

        <view class="padd-lr-30">

            <view class="pro-detial-mess-row">
                <view class="pro-duty-bb">责任除外</view>
                <view class="pro-duty-cc" bindtap="toZerenchuwai">{{exclusion_count}}项</view>
                <view class="clear"></view>
            </view>

            <view class="pro-detial-mess-row">
                <view class="pro-duty-bb">等待期</view>
                <view class="pro-duty-dd">{{waiting_period}}</view>
                <view class="clear"></view>
            </view>

            
            <view class="clear"></view>
        </view>

        <view class="clear"></view>
    </view>

    <include src="../contact-button.wxml"/>

</view>
